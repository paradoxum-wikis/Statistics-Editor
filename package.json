{
  "name": "tds-stats-editor",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "engines": {
    "bun": ">=1.0.0"
  },
  "scripts": {
    "dev": "bunx vite dev",
    "build": "bunx vite build",
    "preview": "bunx vite preview",
    "prepare": "bunx svelte-kit sync || true",
    "check": "bunx svelte-kit sync && bunx svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "bunx svelte-kit sync && bunx svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "bunx prettier --write ."
  },
  "devDependencies": {
    "@internationalized/date": "^3.10.1",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.50.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@tailwindcss/vite": "^4.1.18",
    "bits-ui": "^2.15.4",
    "prettier": "^3.8.1",
    "svelte": "^5.46.4",
    "svelte-check": "^4.3.5",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "dependencies": {
    "@lucide/svelte": "^0.563.1",
    "mediawiki-file-url": "^0.3.0"
  }
}
